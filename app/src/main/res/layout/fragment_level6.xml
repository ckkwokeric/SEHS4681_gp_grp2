<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Mark.Level6Fragment"
    android:layout_marginBottom="@dimen/bottom_nav_min_height">

    <androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Level2Activity">

        <com.example.sehs4681_gp_grp2.Mark.MazeView2_Mark
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/mazeView2" />

        <Button
            android:id="@+id/nextLevelButton2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="You won"
            android:visibility="gone"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>